    "@angular/platform-browser-dynamic": "^4.1.2",
    "@types/express": "^4.0.35",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.47",
    "@types/mime": "^0.0.29",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.12",
    "@types/node": "^7.0.18",
    "@types/serve-static": "^1.7.31",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "@types/socket.io-redis": "^1.0.21",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.1",
    "del": "^2.2.2",
    "del-cli": "^1.0.0",
    "express": "^4.15.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "js.clone": "^0.0.3",
    "json-loader": "^0.5.4",
    "moment": "^2.18.1",
    "mongoose": "^4.9.9",
