    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "5.4.0",
    "sass-loader": "^6.0.5",
