{
  "name": "ngx-socketio-chat-example",
  "version": "1.0.7",
  "description": "An example Angular 2 chat app, made on top of ExpressJS & Socket.io.",
  "homepage": "https://github.com/jussikinnula/",
  "repository": "https://github.com/jussikinnula/angular2-socketio-chat-example",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@gmail.com"
  },
  "license": "GPL-3.0",
  "engines": {
    "node": ">=7",
    "npm": ">=4"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "build": "npm run build:prod",
    "clean": "rimraf target",
    "predev": "npm run clean && npm run build:dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run watch\"",
    "postinstall": "npm run build",
    "server:prod": "node target/server/index.js",
    "server:dev": "nodemon target/server/index.js",
    "server": "npm run server:prod",
    "start": "npm run server",
    "watch": "webpack --config config/webpack.development.ts --watch"
  },
  "dependencies": {
