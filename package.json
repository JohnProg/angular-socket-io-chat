    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
